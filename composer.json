{
    "name": "hawk6/sonata-sortable-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony2 bundle provides sortable and management options to sonata admin lists",
    "keywords": ["Symfony2", "bundle", "admin", "sonata", "sortable"],
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Hawk6\\Bundle\\SonataSortableBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "doctrine/annotations": ">=1.0",
        "doctrine/doctrine-bundle": ">=1.2",
        "doctrine/doctrine-cache-bundle": ">=1.0",
        "doctrine/doctrine-fixtures-bundle": ">=2.2",
        "doctrine/orm": ">=2.4",
        "symfony/assetic-bundle": "^2.6",
        "symfony/monolog-bundle": "^2.4",
        "symfony/symfony": "^2.6",
        "twig/extensions": "^1.2"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": ">=1.5",
        "liip/functional-test-bundle": ">=1.1",
        "mockery/mockery": ">=0.9",
        "phpunit/phpunit": "5.6.0",
        "pdepend/pdepend": ">=2.0",
        "phploc/phploc": ">=2.1",
        "phpunit/php-code-coverage": ">=2.0",
        "sebastian/phpcpd": ">=2.0",
    }
}
